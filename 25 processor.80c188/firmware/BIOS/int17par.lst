    93                              <2> %endm
    94                              <2> 
    95                              <2> %imacro db_lo   1
    96                              <2>  db (%1)&255
    97                              <2> %endm
    98                              <2> 
    99                              <2> %imacro cnop    0.nolist
   100                              <2> %if SOFT_DEBUG
   101                              <2>         nop
   102                              <2> %endif
   103                              <2> %endm
   104                              <2> 
   105                              <2> %imacro popm 1-*.nolist
   106                              <2> %rep %0
   107                              <2> %ifidni %1,ALL
   108                              <2>  popa
   109                              <2> %elifidni %1,F
   110                              <2>  popf
   111                              <2> %else
   112                              <2>  pop %1
   113                              <2> %ifidni %1,DS
   114                              <2>  cnop
   115                              <2> %elifidni %1,ES
   116                              <2>  cnop
   117                              <2> %endif
   118                              <2> %endif
   119                              <2> %rotate 1
   120                              <2> %endrep
   121                              <2> %endm
   122                              <2> 
