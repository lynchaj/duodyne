    71 0000000A 89D3                            MOV     BX,DX                           ; DX is printer index (0 - 3)
    72 0000000C D1E3                            SHL     BX,1                            ;  ...word index
    73 0000000E 8B5708                          MOV     DX,[BX+8]                       ; Load printer port
    74 00000011 09D2                            OR      DX,DX
    75 00000013 740C                            JZ      LP_01                           ; Goes to black hole
    76 00000015 08E4                            OR      AH,AH
    77 00000017 740D                            JZ      LP_02                           ; Function is print, AH=0
    78 00000019 FECC                            DEC     AH
    79 0000001B 7439                            JZ      LP_INI                          ; Function is init , AH=1
    80 0000001D FECC                            DEC     AH
    81 0000001F 7429                            JZ      LP_STS                          ; Get the status   , AH=2
