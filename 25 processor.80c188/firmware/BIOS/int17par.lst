    33                                  ;  BIOS_call_17h  - Print services
    34                                  ;
    35                                  ;	Purpose:  INT 17h BIOS printer services.
    36                                  ;	Entry  :	AH = 0		Print character in AL
    37                                  ;			AH = 1		Initialize port
    38                                  ;			AH = 2		Read printer port status to AH
    39                                  ;			
    40                                  ;			AL = 		character to print
    41                                  ;			DX =		0-based port number
    42                                  ;			All registers preserved except AX
    43                                  ;
    44                                  ;	Exit   :	AH =	Port Status
    45                                  ;				|7|6|5|4|3|2|1|0|  AH (status)
    46                                  ;	 			 | | | | | | | `----  time-out
    47                                  ;	 			 | | | | | `-'------  unused
    48                                  ;	 			 | | | | `--------  1 = I/O error (pin 15)
    49                                  ;	 			 | | | `---------  1 = printer selected/on-line (pin 13)
    50                                  ;	 			 | | `----------  1 = out of paper (pin 12)
    51                                  ;	 			 | `-----------  1 = printer acknowledgment (pin 10)
    52                                  ;	 			 `------------	1 = printer not busy (pin 11)
    53                                  ;
    54                                  ;===============================================================================
    55                                  ;        ENTRY   0EFD2h				; IBM entry for parallel LPT
    56                                  
    57                                  	global	BIOS_call_17h
    58                                  
    59                                  BIOS_call_17h:
    60                                  %if TRACE
    61                                  	call	int_trace
    62                                  %endif	; TRACE
